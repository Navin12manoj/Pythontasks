package main

import "fmt"

func main() {
	var num int = 0
	var rem int = 0
	var rev int = 0
	var tmp int = 0

	fmt.Print("Enter Number: ")
	fmt.Scanf("%d", &num)

	tmp = num
	for num > 0 {
		rem = num % 10
		rev = rev + (rem * rem * rem)
		num = num / 10
 }
	if tmp == rev {
		fmt.Printf("Number is armstrong")
	} else {
		fmt.Printf("Number is not armstrong")
	}
}
